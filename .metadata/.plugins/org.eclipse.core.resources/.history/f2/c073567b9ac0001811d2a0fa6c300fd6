
public class Person {
	private String name=null;//隠蔽
	private int age=0;//隠蔽


	public String getName(){
		return this.name;
	}//カプセル化
	public void setName(String name){
		this.name = name;
	}//カプセル化

	public int getAge(){
		return this.age;
	}
	public void setAge(int age){
		this.age=age;
	}

	public Person(String name, int age){
		this.name=name;
		this.age=age;
	}

}
